
/**分页滚动列表*/
.page_list{
   padding:0px 5px 0px 4px;
}

/**分页页脚*/
.page_bottom{
   text-align: right;
   background-image: url(skins/blue/nav/position_center.jpg);
   background-repeat:repeat-x;
}



/**分页未选中页按钮*/
.page_bottom a{
	border: 1px solid #AAA;
	cursor: pointer;
	border-radius: 2px 2px 2px 2px;/**按钮圆角*/
	margin-left: 3px;
	margin-right: 3px;
	padding-left:2px;
	padding-right:2px;
	color: #333333!important;
	text-align: center;
	font-weight: bold;
	outline: none; 
	background-color: #fff;
	text-decoration:none;
}

.page_bottom div{
	margin-right: 30px;
	font-weight: bold;
}

.page_bottom a:HOVER{
	background-color: #e8f6ff;
}
.page_current span{
	margin-left: 3px;
	margin-right: 3px;
	color: #333333!important;
}
/**分页选中页按钮*/
.page_current{
	background-color:#c5c5c5 !important;;
	cursor: default!important;
}

/**跳转页面input*/
.page_query_input{
	width: 30px;
	height: 18px;
}




/*  ----------  role_list.jsp  -------------------   */
.position .center{        
	background-image: url(nav/position_center.jpg);
	background-repeat:repeat-x;
	width:100%;
	height:100%;
}
.table_bor{
   padding:0px 5px 0px 4px;
}

.box_tool_min{
	height:29px; 
	margin:0px 5px 2px 4px;
}
.table_bot{
   text-align: right;
   margin-left:4px;
   margin-right:4px;
   margin-top:4px;
   padding-right:17px;
   background-image: url(nav/position_center.jpg);
