package com.pd.system.utils;

import java.io.Serializable;

/**
 * 显示信息对象
 * */
public class Vmessage implements Serializable{
	

	private static final long serialVersionUID = -4082299543019716382L;
	//标题
	private String title;
	//信息
	private Object msg;
	//详细信息
	private String detailMsg;
	
	
	public Vmessage() {

	}
	
	public Vmessage(Object msg) {
		this.msg = msg;
	}
	
	public Vmessage(String title, Object msg, String detailMsg) {
		this.title = title;
		this.msg = msg;
		this.detailMsg = detailMsg;
	}


	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public Object getMsg() {
		return msg;
	}
	public void setMsg(Object msg) {
		this.msg = msg;
	}
	public String getDetailMsg() {
		return detailMsg;
	}
	public void setDetailMsg(String detailMsg) {
		this.detailMsg = detailMsg;
	}
	
}
