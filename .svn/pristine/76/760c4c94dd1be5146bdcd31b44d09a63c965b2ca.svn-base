  <select id="getUser" parameterClass="java.util.HashMap" resultClass="com.pd.right.model.SuperUser">
    select a.USER_CODE as userCode, a.USER_NAME as userName, a.NICK_NAME as nickName, a.PASSWORD, a.USER_GENDER as userGender, a.BORN_DATE as bornDate, a.CREATE_DATE as createDate, a.USERSTAT,
