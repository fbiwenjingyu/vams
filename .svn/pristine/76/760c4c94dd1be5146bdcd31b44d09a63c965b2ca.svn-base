      a.LAST_LOGIN_TIME as lastLoginTime, a.LAST_LOGIN_IP as lastLoginIp, a.ROLE_ID as roleId, a.IS_ONLINE as isOnline, a.LOGIN_TYPE as loginType, a.PYM, a.PHOTO, a.GWH, a.F1, a.F2, a.F3,
      a.F4, a.F5,b.deptarea,b.deptname,c.rolename,c.f2 as sftz,d.levels  
    from USERS a,DEPT b ,ROLE c,SECLEVEL d 
    where a.DEPT_CODE = b. DEPTCODE 
    and a.ROLE_ID = ROLEID 
    and c.LEVEL_ID = d.id
    and USER_CODE=#userCode# 
