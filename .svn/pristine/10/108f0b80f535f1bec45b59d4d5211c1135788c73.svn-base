<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<!-- spring管理struts的的配置文件-->
	
	<bean id="stoSettingAction" class="com.pd.cwgl.action.StoSettingAction" scope="prototype">
		<property name="stoService" ref="stoService"/>
	</bean>
	<bean id="stoGettingAction" class="com.pd.cwgl.action.StoGettingAction" scope="prototype">
		<property name="stoService" ref="stoService"/>
	</bean>
	<bean id="stoQueryAction" class="com.pd.cwgl.action.StoQueryAction" scope="prototype">
		<property name="stoQueryService" ref="stoQueryService"/>
	</bean>
</beans>