package com.pd.cwgl.utils;

import com.pd.system.startup.DBResource;

/**
 * 储位管理 必备属性
 * */
public class CwglConsts {

	/**
	 * 有效、可用
	 * */
	public static final String ENABLE = "1";
	/**
	 * 无效、不可用
	 * */
	public static final String DISABLE = "0";
	/**
	 * 当前的
	 * */
	public static final String CURRENT="2";
	/**
	 * 被占满、满的
	 * */
	public static final String FULL="3";
	
	/**
	 * 存放类型：未设定说明
	 * */
	public static final String CFLX_SM="未设定说明";

	/**
	 * 储位类型：室
	 * */
	public static final String CWLX_SHI = "1";
	/**
	 * 储位类型：柜
	 * */
	public static final String CWLX_GUI = "2";
	/**
	 * 储位类型：列
	 * */
	public static final String CWLX_LIE = "3";
	/**
	 * 储位类型：格
	 * */
	public static final String CWLX_GE = "4";

	/**
	 * 默认容量：室
	 * */
	public static int STO_SHI_RL = Integer.parseInt(DBResource
			.getSysAttrName("sto_shi_rl"));

	/**
	 * 默认容量：柜
	 * */
	public static int STO_GUI_RL = Integer.parseInt(DBResource
			.getSysAttrName("sto_gui_rl"));

	/**
	 * 默认容量：列
	 * */
	public static int STO_LIE_RL = Integer.parseInt(DBResource
			.getSysAttrName("sto_lie_rl"));

	/**
	 * 默认容量：格
	 * */
	public static int STO_GE_RL = Integer.parseInt(DBResource
			.getSysAttrName("sto_ge_rl"));


}
