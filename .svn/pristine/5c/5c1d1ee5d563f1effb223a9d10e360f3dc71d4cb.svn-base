/*基本样式*/
*{
  margin:0px;
  padding:0px;
}

body{
	text-align: left;
	background-color: #dbeefd;
	color:#3f3f3f;
	font-size: 12px;
	font-family: :Arial, Helvetica, sans-serif;
}
a{
	color: #333333;
	text-decoration: none;
	outline:none;
	cursor: hand;
	cursor: pointer;
}
a:hover{
	color: #4a88df;
	text-decoration: underline;
}
/*基本样式*/

/*框架主体设置*/
#mainFrame{
	background-color:#e8f6ff; /*主体内容区颜色*/
}
.contentStyleLeft{
	padding: 0 0px 0 6px; /*设置左侧内容边距*/
}
.contentStyle{
	padding: 0 0px 0 4px; /*设置右侧内容边距*/
}
#hideCon{
	width: 173px; /*设置左侧框架宽度*/
	background-color: rgb(25,139,201);
}
#bs_left{
	border-left:6px solid rgb(25,139,201);
	width: 167px; /*设置左侧框架宽度*/

}
/*框架主体设置*/

/*设置左侧伸缩的按钮样式*/
.bs_leftArr{
	width: 6px;/*竖条宽度*/
	height: 39px;/*竖条高度*/
	background-image: url(4_16.gif);
	background-repeat: no-repeat;
	cursor: pointer;
	cursor: hand;
}
.bs_rightArr{
	width: 6px;/*竖条宽度*/
	height: 39px;/*竖条高度*/
	background-image: url(4_17.gif);
	background-repeat: no-repeat;
	cursor: pointer;
	cursor: hand;
}
.main_shutiao{
	width: 6px;/*竖条容器宽度*/
	height: 100%;
	background-image: url(4_18.gif);
	background-repeat: repeat-y;
}
/*设置左侧伸缩的按钮样式*/

/*框架左侧盒子模型*/
.lbox_title{
	display: none;/*当左侧盒子标题为图片时隐藏文字标题*/
}
.lbox_foot{
	display: none;/*左侧盒子底部空间小时隐藏版本说明*/
}
#lbox_topcenter{
	height:0;
	overflow: hidden;
}
#lbox_bottomcenter{
	height:0;
	overflow: hidden;
}
/*框架左侧盒子模型*/

/*框架右侧盒子模型*/
.rbox_title{
	display: none;/*当右侧盒子标题为图片时隐藏文字标题*/
}
#rbox_topcenter{
	height:0;
	overflow: hidden;
}
#rbox_bottomcenter{
	height:0;
	overflow: hidden;
}
/*框架右侧盒子模型*/

